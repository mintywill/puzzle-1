var steps = 0
while (steps < 60) {
  setColor("red")
  down();
  steps = steps + 1
}
var steps = 0
while (steps < 60) {
  setColor("red")
  right();
  steps = steps + 1
}
var steps = 0
while (steps < 60) {
  setColor("red")
  up();
  steps = steps + 1
}
var steps = 0
while (steps < 60) {
  setColor("red")
  left();
  steps = steps + 1
}
